      <editor
            id="fr.unice.polytech.robotproject.model.RobotProjectModel.presentation.RobotProjectModelEditorID"
            name="%_UI_RobotProjectModelEditor_label"
            icon="icons/full/obj16/RobotProjectModelModelFile.gif"
            extensions="robotprojectmodel"
            class="fr.unice.polytech.robotproject.model.RobotProjectModel.presentation.RobotProjectModelEditor"
            contributorClass="fr.unice.polytech.robotproject.model.RobotProjectModel.presentation.RobotProjectModelActionBarContributor">
