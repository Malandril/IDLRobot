            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.unice.polytech.robotproject.model.RobotProjectModel.presentation.RobotProjectModelModelWizardID"
